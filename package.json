{
  "name": "expense_tracker",
  "version": "1.0.0",
  "description": "An application that allows users to keep trak of their expenses",
  "main": "server.js",
  "scripts": {
    "build-client": "rimraf server/dist && webpack --mode production --config webpack.config.dev.js",
    "build-server": "rimraf build && babel server -d build --copy-files",
    "build": "cross-env NODE_ENV=production npm run build-client && npm run build-server",
    "client": "webpack-dev-server --config webpack.config.dev.js",
    "server": "nodemon server/server.js --exec babel-node",
    "dev-server": "cross-env NODE_ENV=development concurrently \"npm run client\" \"npm run server\" ",
    "start": "npm run build && node build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChinedumIfeanyi/expense_tracker.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChinedumIfeanyi/expense_tracker/issues"
  },
  "homepage": "https://github.com/ChinedumIfeanyi/expense_tracker#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "helmet": "^3.15.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mysql": "^2.16.0",
    "vue": "^2.6.2",
    "vue-router": "^3.0.2",
    "vue-swal": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.9",
    "rimraf": "^2.6.3",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.2",
    "webpack": "^4.29.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  }
}
